[sub_resource type="GDScript" id="GDScript_fi7a1"]
script/source = "extends CanvasLayer
@export var cena_atual:String
@export var cena_pos:String

func _physics_process(_delta: float) -> void:
	if $Color/Anim.current_animation == \"In\":
		if cena_atual == \"res://day transition.tscn\":
			cena_pos = \"res://Scenes/officetable.tscn\"
			get_tree().change_scene_to_file(cena_pos)
		elif cena_atual == \"res://Scenes/officetable.tscn\":
			cena_pos = \"res://day transition.tscn\"
			get_tree().change_scene_to_file(cena_pos)
		$Color/Anim.play(\"Out\")
"

